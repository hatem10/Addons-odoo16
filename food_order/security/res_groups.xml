<odoo>
    <data>
        <!--Category-->
        <record id="category_food_order" model="ir.module.category">
            <field name="name">Pedidos de Comida</field>
        </record>

        <!--Groups-->
        <record id="group_admin_food_order" model="res.groups">
            <field name="name">Administrador</field>
            <field name="category_id" ref="category_food_order"/>
        </record>

        <record id="group_user_food_order" model="res.groups">
            <field name="name">Usuario</field>
            <field name="category_id" ref="category_food_order"/>
        </record>
    </data>
</odoo>